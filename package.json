    "discord.js": "github:discordjs/discord.js",
    "discord.js-docgen": "github:distubejs/docgen",
    "eslint": "^7.25.0",
    "eslint-plugin-jsdoc": "^33.1.0",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
