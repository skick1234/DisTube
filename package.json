    "discordjs",
    "bot",
    "distube",
    "queue",
    "musicbot",
    "discord-music-bot",
    "music-bot",
    "discord-js"
  ],
  "author": "Skick (https://github.com/skick1234/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skick1234/DisTube/issues"
  },
  "funding": {
    "url": "https://patreon.com/DisTube"
  },
  "homepage": "https://distube.js.org/",
  "dependencies": {
    "@distube/youtube-dl": "^2.1.1",
    "@distube/ytdl": "^2.0.6",
    "@distube/ytpl": "^1.0.11",
    "@distube/ytsr": "^1.0.9",
    "ytdl-core": "^4.7.0"
  },
  "devDependencies": {
