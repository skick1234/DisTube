    "lint:fix": "eslint . --fix",
    "types": "tsc -p .",
    "publish:beta": "npm publish --tag beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skick1234/DisTube.git"
  },
  "keywords": [
    "youtube",
    "music",
